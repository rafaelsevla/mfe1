{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES5",
    "outDir": "dist",
    "rootDir": "src",
    "jsx": "react",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "sourceMap": true
  }
}
